{
    "name": "dag-ag-tracker",
    "version": "1.0.0",
    "description": "Monitor and track dag-ag infestations to protect coral reefs",
    "author": "Dag-ag Tracker Team",
    "private": true,
    "type": "module",
    "main": "public/index.php",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "mobile:build": "npm run build && npx cap sync",
        "mobile:android": "npm run mobile:build && npx cap open android",
        "mobile:ios": "npm run mobile:build && npx cap open ios",
        "mobile:add:android": "npx cap add android",
        "mobile:add:ios": "npx cap add ios",
        "mobile:sync": "npx cap sync",
        "mobile:copy": "npx cap copy",
        "apk:build": "npm run mobile:build && cd android && ./gradlew assembleDebug",
        "apk:release": "npm run mobile:build && cd android && ./gradlew assembleRelease"
    },
    "devDependencies": {
        "@capacitor/android": "^6.0.0",
        "@capacitor/cli": "^6.0.0",
        "@capacitor/core": "^6.0.0",
        "@capacitor/ios": "^6.0.0",
        "@capacitor/camera": "^6.0.0",
        "@capacitor/geolocation": "^6.0.0",
        "@capacitor/filesystem": "^6.0.0",
        "@capacitor/network": "^6.0.0",
        "@popperjs/core": "^2.11.6",
        "@vitejs/plugin-vue": "^4.5.0",
        "axios": "^1.6.4",
        "bootstrap": "^5.2.3",
        "laravel-vite-plugin": "^1.0.0",
        "sass": "^1.56.1",
        "vite": "^5.0.0",
        "vue": "^3.2.37"
    },
    "dependencies": {
        "workbox-background-sync": "^7.0.0",
        "workbox-cacheable-response": "^7.0.0",
        "workbox-core": "^7.0.0",
        "workbox-expiration": "^7.0.0",
        "workbox-google-analytics": "^7.0.0",
        "workbox-navigation-preload": "^7.0.0",
        "workbox-precaching": "^7.0.0",
        "workbox-range-requests": "^7.0.0",
        "workbox-routing": "^7.0.0",
        "workbox-strategies": "^7.0.0",
        "workbox-streams": "^7.0.0"
    }
}
